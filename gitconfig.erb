[core]
  editor = vim
[alias]
  ci = commit
  co = checkout
  st = status
  br = branch
  lg = "log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
[push]
  default = simple
[pull]
  rebase = true
[color]
  ui = true
[diff]
  algorithm = patience
[protocol]
  version = 2
[credential]
  helper = osxkeychain
